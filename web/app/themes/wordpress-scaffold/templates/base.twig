<!doctype html>
<html class="no-js" {{ site.language_attributes }}>
    {% include 'partials/head.twig' %}

    <body class="{{ body_class }}">
        {% include 'partials/tracking/google-tag-manager-noscript.twig' %}

        <a class="skiplink" href="#content">
            {{ __( 'Skip to content') }}
        </a>

        {% include 'partials/site-header.twig' %}

        <main class="site-wrapper" id="content">
            {% include templates %}
        </main>

        {% include 'partials/site-footer.twig' %}
        {{ function('wp_footer') }}

        <!--[if IE]>
        <div class="browser-warning">
            {{ __('You are using a <strong>strongly outdated</strong> browser. Please <a href="http://outdatedbrowser.com/" target="_blank">upgrade your browser</a> to improve your experience.', 'grrr')|raw }}
        </div>
        <![endif]-->

        {% if option('enabled', 'cookie consent') %}
            {% include 'partials/cookie-consent.twig' %}
        {% endif %}
    </body>
</html>

<!--

Version: {{ constant('APPLICATION_VERSION') ?: 'undefined' }}

Performance: {{ function('get_num_queries') }} queries in {{ function('timer_stop') }} seconds

-->
