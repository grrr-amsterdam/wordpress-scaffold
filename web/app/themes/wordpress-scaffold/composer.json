{
  "name": "grrr/wordpress-scaffold",
  "type": "wordpress-theme",
  "description": "WordPress starter theme with a modern development workflow",
  "license": "BSD-3-Clause",
  "homepage": "https://grrr.nl/",
  "authors": [
    {
      "name": "Ramiro Hammen",
      "email": "ramiro@grrr.nl"
    },
    {
      "name": "Koen Schaft",
      "email": "koen@grrr.nl"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Grrr\\": [
        "lib/Grrr/"
      ]
    },
    "files": [
      "lib/Grrr/Utils/Cleanup.php",
      "lib/Grrr/Utils/Debug.php",
      "lib/Grrr/Utils/Utils.php",
      "lib/Sage/Assets.php",
      "lib/Sage/Extras.php",
      "lib/Sage/Setup.php",
      "lib/Sage/Wrapper.php"
    ]
  },
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.2.0"
  }
}
